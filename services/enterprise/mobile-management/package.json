{
  "name": "@opendirectory/mobile-management-suite",
  "version": "1.0.0",
  "description": "Enterprise Mobile Management Suite for OpenDirectory UEM Platform",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:ios": "node ios-management-service.js",
    "start:android": "node android-enterprise-service.js",
    "start:mam": "node mobile-app-management-service.js",
    "start:mtd": "node mobile-threat-defense-service.js",
    "start:all": "concurrently \"npm run start:ios\" \"npm run start:android\" \"npm run start:mam\" \"npm run start:mtd\"",
    "dev": "concurrently \"nodemon ios-management-service.js\" \"nodemon android-enterprise-service.js\" \"nodemon mobile-app-management-service.js\" \"nodemon mobile-threat-defense-service.js\"",
    "test": "jest",
    "lint": "eslint *.js",
    "docker:build": "docker build -t opendirectory/mobile-management .",
    "docker:run": "docker run -p 3011:3011 -p 3012:3012 -p 3013:3013 -p 3014:3014 opendirectory/mobile-management"
  },
  "keywords": [
    "mobile",
    "device-management",
    "mdm",
    "mam",
    "mtd",
    "ios",
    "android",
    "enterprise",
    "uem",
    "security"
  ],
  "author": "OpenDirectory Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "ws": "^8.13.0",
    "axios": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "crypto": "^1.0.1",
    "multer": "^1.4.5-lts.1",
    "googleapis": "^128.0.0",
    "aws-sdk": "^2.1479.0",
    "node-cron": "^3.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.53.0",
    "@types/node": "^20.8.10"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opendirectory/mobile-management.git"
  },
  "bugs": {
    "url": "https://github.com/opendirectory/mobile-management/issues"
  },
  "homepage": "https://github.com/opendirectory/mobile-management#readme"
}